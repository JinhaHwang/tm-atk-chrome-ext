{
  "name": "tm-atk",
  "description": "",
  "version": "1.0.0",
  "browser_action": {
    "default_popup": "hello.html",
    "default_icon": "hello.png"
  },
  "permissions": [
    "<all_urls>"
  ],
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "description": "Opens hello.html"
    }
  },
  "web_accessible_resources": ["lib/atk.min.js"],
  "content_scripts": [
    {
      "matches": [
        "*://*.wemakeprice.com/*"
      ],
      "run_at": "document_end",
      "js": [
        "contentsScript.min.js"
      ]
    }
  ]
}
